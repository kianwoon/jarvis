      "Bash(log show:*)",
      "Bash(ps:*)",
      "Bash(docker logs:*)",
      "Bash(docker inspect:*)",
      "Bash(docker ps:*)",
      "Bash(docker-compose logs:*)",
      "Bash(PGPASSWORD=postgres psql -h localhost -U postgres -l)",
      "Bash(PGPASSWORD=postgres psql -h localhost -U postgres -d *)",
