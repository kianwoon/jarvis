  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "say 'Claude needs your input' && osascript -e 'display notification \"Claude is waiting for a response\" with title \"Claude Code\" sound name \"Ping\"'"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "say 'Task completed' && osascript -e 'display notification \"Task completed successfully\" with title \"Claude Code\" sound name \"Submarine\"'"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "agent_name=$(echo \"${SUBAGENT_NAME:-Unknown}\"); say \"${agent_name} complete\" && osascript -e 'display notification \"Subtask completed\" with title \"Claude Code\" sound name \"Glass\"'"
          }
        ]
      }
    ],
